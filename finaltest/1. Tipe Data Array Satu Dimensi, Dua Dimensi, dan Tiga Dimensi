#===Array 1 Dimensi===
buah = ["mangga", "durian", "semangka", "rambutan"]
print("Buah ke-2:", buah[1])

#===Array 2 Dimensi===
nilai_kelas = [
    [87, 93, 79],
    [88, 91, 82],
    [75, 85, 90]
]
print("Nilai baris ke-2 kolom ke-3:", nilai_kelas[1][2])

#===Array 3 Dimensi===
data_toko = [
    [
        ["susu", 10], 
        ["roti", 20]
    ],
    [
        ["sabun", 15], 
        ["shampoo", 12]
    ]
]
print("Nama barang:", data_toko[0][1][0])


# Output
#Buah ke-2: durian
#Nilai baris ke-2 kolom ke-3: 82
#Nama barang: roti
