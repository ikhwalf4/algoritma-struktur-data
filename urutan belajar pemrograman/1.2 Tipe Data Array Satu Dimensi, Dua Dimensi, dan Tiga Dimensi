# Array 1 dimensi (list biasa)
angka = [10, 20, 30, 40, 50]
print(angka[2])  # akses elemen indeks ke-2

# Array 2 dimensi (list dalam list)
matriks = [
    [10, 20, 30],
    [40, 50, 60]
]

print(matriks[1][2])  # akses baris 1, kolom 2

# Array 3 dimensi
kubus = [
    [
        [1, 2], 
        [3, 4]
    ],
    [
        [5, 6],
        [7, 8]
    ]
]

print(kubus[1][0][1])  # lapisan 1, baris 0, kolom 1
